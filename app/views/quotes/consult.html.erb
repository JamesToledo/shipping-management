<section style="text-align:center">
  <div><h1>Preecha os campos abaixo</h1></div>
  <div>
    <%= form_with(url: search_quotes_path, data: { turbo: false }) do |form| %>
      <div>
        <%= form.label :height, "#{t'.height'}" %><br />
        <%= form.number_field :height %>
      </div>
      <div>
        <%= form.label :width, "#{t'.width'}" %><br />
        <%= form.number_field :width %>
      </div>
      <div>
        <%= form.label :length, "#{t'.length'}" %><br />
        <%= form.number_field :length %>
      </div>
      <div>
        <%= form.label :weight , "#{t'.weight'}"%><br />
        <%= form.number_field :weight %>
      </div>
      <div>
        <%= form.label :space, "#{t'.space'}" %><br />
        <%= form.number_field :space %>
      </div>
      <div>
        <%= form.submit 'Consultar' %>
      </div>
    <% end %>
  </div>
</section>
